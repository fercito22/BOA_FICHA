        <div class="section-title">
            <h4 class="text-uppercase text-center"><i class="title-icon fa fa-suitcase"></i>Beneficiarios</h4>
        </div>

        <div id="timeline-experience">
                {{#each webapidataI as |item|}} 
                <!-- FIRST TIMELINE -->
                <div class="timeline-block">
                    <!-- Circulo con la primer Letra  -->
                    {{!-- <div class="timeline-dot"><h6>Letra</h6></div> --}}
                
                    <!-- TIMELINE CONTENT -->
                    <div class="card timeline-content">
                        <div class="card-content">
                           
                            <!-- TIMELINE TITLE -->
                            <h5 class="text-uppercase ">{{item.Nombres}} {{item.Apellido1}} {{item.Apellido2}}</h5>
                            <hr id="linea2"> 
                            <!-- TIMELINE TITLE INFO -->
                            <div class="timeline-info">                                
                                <h6>
                                    N° Documento: <small>{{item.Tipo_NroDocumento}}</small>
                                </h6>
                                <h6>
                                    Beneficiario: <small>{{item.Beneficiario}}</small>
                                </h6>
                                <h6>
                                    Finado: <small>{{item.Finado}}</small>
                                </h6>
                                <h6>
                                    Parentesco: <small>{{item.TipoParentesco}}</small>
                                </h6>
                            </div>
                            
                            <!-- TIMELINE PARAGRAPH -->
                           <button id="botonEditar" type="button" style="float:right; margin-right: 1%; margin-block-start: -1%; " class="btn" data-toggle="modal" data-target="#exampleModal">
                                {{fa-icon 'user-edit'}} Editar
                            </button> 
                        </div>
                    </div>
                </div>
                {{/each}}                              
        </div>
    </div>

