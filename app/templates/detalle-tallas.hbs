{{!-- style="width: 800px; border:2px solid red " --}}
<div >
    
<div class="modal-dialog" role="document" style="background: #2F4765; width:800px; ">
    <div class="modal-content">
      <div class="modal-header" style="background: #2F4765">
        <h5 class="modal-title" id="exampleModalLabel" style="color: white">Detalle Tallas</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" style="background: #EEEEEE">
        <form style="background: whitesmoke; margin: 2%" >
            <h5 class="text-uppercase ">Gestion</h5>
            <select  class="form-control" id="sel3" onchange={{action 'gestionSelected' value="target.value"}} >
                {{#each ComboTallasDetalle as |item|}} 
                    <option value={{item.ControlDotacionID}}  selected={{is-eq item.FechaInicial fechaActual}} >{{item.FechaInicial}}</option>
                {{/each}}                            
            </select>
            <hr id="linea2"> 

            {{#each objeto2 as |item|}} 
                <h5 style="margin: 2%"  class="text-uppercase ">{{item.CodigoTran}}</h5>
                <hr id="linea2">
                <div class=" " >
                    <div class="card-content">
                        <div class="timeline-info" style="margin: 4%">                              
                            <h6>                        
                                <span class="title">EsExtra: <span id="negrita">{{item.EsExtra}}</span></span>
                            </h6>                    
                            <h6>
                                <span class="title">Fecha Dotacion: <span id="negrita">{{item.fechaMov}}</span></span>                        
                            </h6>
                            {{!-- <h6>
                                <span class="title">Nombre de Prenda: <span id="negrita">{{item.prenda}}</span></span>
                            </h6>                --}}
                            <br>
                        </div> 
                    </div>                    
                </div> 
                
                {{!-- <h6>{{item.CantidadMovimiento}}</h6>
                <h6>{{item.CodigoTran}}</h6>
                <h6>{{item.fechaMov}}</h6>
                <h6>{{item.fechaAprobacion}}</h6>
                <h6>{{item.prenda}}</h6> --}}
                {{render}}
            {{/each}} 

        
      </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        {{!-- {{#link-to 'index' tagName="button" class="btn btn-secondary"}}
            Regresar
        {{/link-to}} --}}
        {{!-- <button type="button" disabled={{isDisabledBeneficiario}} class="btn btn-primary" {{action 'onGuardar'}}>Guardar</button> --}}
      </div>
    </div>
  </div>


</div>



